{
  "name": "react-native-render-html-table-bridge",
  "version": "0.6.2-alpha.3",
  "description": "A bridge to render tables with react-native-render-html library",
  "main": "lib/index.js",
  "types": "types/react-native-webshell/index.d.ts",
  "files": [
    "lib/**/*",
    "types/**/*"
  ],
  "scripts": {
    "build": "rimraf lib; tsc --project ./tsconfig.json && api-extractor run --local --verbose && build:doc",
    "prepare": "npm run build",
    "lint": "tslint 'src/**/*?(.d).ts?(x)' --fix",
    "build:doc": "api-documenter markdown --input-folder temp --output-folder doc"
  },
  "keywords": [
    "react native",
    "html",
    "table"
  ],
  "author": "Jules Sam. Randolph",
  "license": "MIT",
  "repository": "git@github.com:jsamr/react-native-render-html-table-bridge.git",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.21",
    "@microsoft/api-extractor": "^7.9.2",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/react": "^16.9.41",
    "@types/react-native": "^0.62.16",
    "react": "^16.11.0",
    "react-native": "^0.62.2",
    "react-native-render-html": "^4.2.1",
    "react-native-webview": "^10.3.2",
    "rimraf": "^2.7.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react": "^4.1.0",
    "tslint-react-recommended": "^1.0.15",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-native-webshell": "^2.0.0-alpha.5",
    "stringify-entities": "^2.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-render-html": ">= 4.2.1 < 5.0"
  }
}
